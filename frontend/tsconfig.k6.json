{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "lib": ["ES2015"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["k6"],
    "noEmit": false,
    "outDir": "./dist"
  },
  "include": ["tests/**/*.ts"],
  "exclude": ["node_modules", "src", "dist"]
}